<div class="user-<%= user.id %>">
  <div class="row">
    <div class="col-md-9">
      <%= image_tag user.avatar.url if user.avatar? %>
      <%= link_to user.name, user %>
    </div>
    <% unless user.admin? %>
      <div class="col-md-1">
        <%= link_to "Delete", admin_user_path(user),
            method: :delete %>
      </div>
    <% end %>
    <div class="col-md-1">
      <%= link_to "Edit", edit_admin_user_path(user) %>
    </div>
  </div>
</div>
<br>
